#include		"shunting_yard.h"
#include		"my_flags.h"



const int		evaluate_postfix(t_op *operation)
{
  t_shunting		stack;

  if ((stack.operator_stack = new_linked_list()) == NULL)
    return (RETURN_FAILURE);



  free(shunting.operator_stack);
  return (RETURN_SUCCESS);
}
